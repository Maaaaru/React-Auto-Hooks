{
	"name": "React-Auto-Hooks",
	"displayName": "React-Auto-Hooks",
	"description": "React Hooks Expansion",
	"author": "maaaaru",
	"version": "1.1.0",
	"publisher": "maaaaru",
	"repository": {
		"type": "git",
		"url": "https://github.com/Maaaaru/React-Auto-Hooks.git"
	},
	"icon": "images/logo.png",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Snippets"
	],
	"keywords": [
		"vscode",
		"snippets",
		"typescript",
		"react",
		"react native",
		"hooks"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:extension.useState",
		"onCommand:extension.useEffect",
		"onCommand:extension.useContext",
		"onCommand:extension.useRef",
		"onCommand:extension.useReducer",
		"onCommand:extension.useMemo",
		"onCommand:extension.useCallback",
		"onCommand:extension.useImperativeHandle"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.useState",
				"title": "ust"
			},
			{
				"command": "extension.useEffect",
				"title": "uff"
			},
			{
				"command": "extension.useContext",
				"title": "uct"
			},
			{
				"command": "extension.useRef",
				"title": "urf"
			},
			{
				"command": "extension.useReducer",
				"title": "urd"
			},
			{
				"command": "extension.useMemo",
				"title": "umo"
			},
			{
				"command": "extension.useCallback",
				"title": "ucb"
			},
			{
				"command": "extension.useImperativeHandle",
				"title": "uih"
			}
		],
		"keybindings": [
			{
				"command": "extension.useState",
				"mac": "shift+cmd+u",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useEffect",
				"mac": "shift+cmd+f",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useContext",
				"mac": "shift+cmd+c",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useRef",
				"mac": "shift+cmd+r",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useReducer",
				"mac": "shift+cmd+d",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useMemo",
				"mac": "shift+cmd+m",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useCallback",
				"mac": "shift+cmd+b",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.useImperativeHandle",
				"mac": "shift+cmd+i",
				"when": "editorTextFocus"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./src/snippets/snippets.json"
			},
			{
				"language": "javascriptreact",
				"path": "./src/snippets/snippets.json"
			},
			{
				"language": "typescript",
				"path": "./src/snippets/snippets.json"
			},
			{
				"language": "typescriptreact",
				"path": "./src/snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
